<Window x:Class="Vista.CrearContrato"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Vista"
        mc:Ignorable="d"
        Title="Ingresar Contrato" Height="300" Width="300">
    <Grid>
        <GroupBox x:Name="groupBox" Header="Datos del Contrato" Margin="10">
            <Grid Margin="10,10,-2,3">
                <Label x:Name="label" Content="Label" HorizontalAlignment="Left" Margin="10,20,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.278,0.077"/>
            </Grid>
        </GroupBox>

    </Grid>
</Window>
